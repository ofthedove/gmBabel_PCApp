<Window x:Class="gmBabel_PCApp.CharacterDeleteWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:gmBabel_PCApp"
        mc:Ignorable="d"
        Title="Delete Character" Height="106.843" Width="243.235" ResizeMode="NoResize">
    <Grid>
        <ComboBox x:Name="comboBox" Margin="10,10,10,0" VerticalAlignment="Top"/>
        <Button x:Name="deleteButton" Content="Delete" Margin="0,0,10,10" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75"/>
        <Button x:Name="cancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="10,0,0,10" VerticalAlignment="Bottom" Width="74" IsCancel="True"/>

    </Grid>
</Window>
